<Window x:Class="DiffSync.TestApp.ClientWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiffSync.TestApp"
        mc:Ignorable="d"
        Title="ClientWindow" Height="450" Width="800">
    <Grid Loaded="Grid_Loaded">
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="137" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="275" TextChanged="TextBox_TextChanged"/>
        <local:DocumentView x:Name="ServerShadowView" HorizontalAlignment="Left" Margin="400,10,0,0" VerticalAlignment="Top" Height="137" Width="390"/>

    </Grid>
</Window>
