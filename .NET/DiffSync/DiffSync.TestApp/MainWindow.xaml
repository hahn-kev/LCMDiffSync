<Window x:Class="DiffSync.TestApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiffSync.TestApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="446" Width="845">
    <Grid HorizontalAlignment="Left" Width="904">
        <TextBox x:Name="serverText" HorizontalAlignment="Left" Height="312" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="299" TextChanged="TextBox_TextChanged"/>
        <Button x:Name="addClient" Content="Add Client" HorizontalAlignment="Left" Height="42" Margin="329,10,0,0" VerticalAlignment="Top" Width="91" Click="addClient_Click"/>
        <local:DocumentView x:Name="ClientViews" HorizontalAlignment="Left" Height="309" Margin="433,13,0,0" VerticalAlignment="Top" Width="403" />

    </Grid>
</Window>
